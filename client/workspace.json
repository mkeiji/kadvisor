{
    "version": 1,
    "projects": {
        "kadvisor-app": {
            "root": "apps/kadvisor-app",
            "sourceRoot": "apps/kadvisor-app/src",
            "projectType": "application",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "options": {
                        "outputPath": "dist/apps/kadvisor-app",
                        "index": "apps/kadvisor-app/src/index.html",
                        "main": "apps/kadvisor-app/src/main.tsx",
                        "polyfills": "apps/kadvisor-app/src/polyfills.ts",
                        "tsConfig": "apps/kadvisor-app/tsconfig.app.json",
                        "assets": [
                            "apps/kadvisor-app/src/favicon.ico",
                            "apps/kadvisor-app/src/assets"
                        ],
                        "styles": ["apps/kadvisor-app/src/styles.css"],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/kadvisor-app/src/environments/environment.ts",
                                    "with": "apps/kadvisor-app/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "4mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "kadvisor-app:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "kadvisor-app:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "apps/kadvisor-app/tsconfig.app.json",
                            "apps/kadvisor-app/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/kadvisor-app/**/*"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/kadvisor-app/jest.config.js",
                        "tsConfig": "apps/kadvisor-app/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "kadvisor-app-e2e": {
            "root": "apps/kadvisor-app-e2e",
            "sourceRoot": "apps/kadvisor-app-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/kadvisor-app-e2e/cypress.json",
                        "tsConfig": "apps/kadvisor-app-e2e/tsconfig.e2e.json",
                        "devServerTarget": "kadvisor-app:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "kadvisor-app:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": ["apps/kadvisor-app-e2e/tsconfig.e2e.json"],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/kadvisor-app-e2e/**/*"
                        ]
                    }
                }
            }
        },
        "klibs": {
            "root": "libs/klibs",
            "sourceRoot": "libs/klibs/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "libs/klibs/tsconfig.lib.json",
                            "libs/klibs/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!libs/klibs/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/klibs/jest.config.js",
                        "tsConfig": "libs/klibs/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "style": "css",
                "linter": "eslint",
                "babel": true
            },
            "library": {
                "style": "css",
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            },
            "component": {
                "style": "css"
            }
        },
        "@nrwl/next": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/web": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/node": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/nx-plugin": {
            "plugin": {
                "linter": "eslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "kadvisor-app"
}
